{
  "parser": "babel-eslint",
  "extends": "react",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "indent": "off",
    "space-in-parens": 1,
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "react/jsx-wrap-multilines": "off"
  }
}
